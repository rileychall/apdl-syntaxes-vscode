{
    "name": "apdl",
    "displayName": "APDL",
    "description": "APDL(ANSYS Parametric Design Language) Language Support for VS Code.",
    "version": "0.1.2",
    "publisher": "shenbo",
    "engines": {
        "vscode": "^1.23.1"
    },
    "icon": "images/logo-ansys.jpg",
    "categories": [
        "Languages",
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/shenbo/apdl-syntaxes-vscode"
    },
    "contributes": {
        "languages": [
            {
                "id": "apdl",
                "aliases": [
                    "APDL (ANSYS Parametric Design Language)",
                    "apdl"
                ],
                "extensions": [
                    ".mac",
                    ".ans",
                    ".ansys.txt",
                    ".ans.txt",
                    ".ansys",
                    ".apdl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "apdl",
                "scopeName": "source.apdl",
                "path": "./syntaxes/apdl.tmLanguage.json"
            }
        ]
    }
}
