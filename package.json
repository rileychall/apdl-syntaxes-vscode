{
    "name": "apdl-syntax-highlighting",
    "displayName": "APDL Syntax Highlighting",
    "description": "APDL(ANSYS Parametric Design Language) Language Support for VS Code.",
    "version": "0.1.3",
    "publisher": "RileyHall",
    "engines": {
        "vscode": "^1.23.1"
    },
    "icon": "images/logo-ansys.jpg",
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/rileychall/apdl-syntaxes-vscode"
    },
    "contributes": {
        "languages": [
            {
                "id": "apdl",
                "aliases": [
                    "APDL (ANSYS Parametric Design Language)",
                    "apdl"
                ],
                "extensions": [
                    ".mac",
                    ".ans",
                    ".ansys.txt",
                    ".ans.txt",
                    ".ansys",
                    ".apdl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "apdl",
                "scopeName": "source.apdl",
                "path": "./syntaxes/apdl.tmLanguage.json"
            }
        ]
    }
}
